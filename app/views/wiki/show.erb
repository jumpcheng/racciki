<h1><%= @wiki.title %></h1>

<div class="wiki_content">
  <%= @wiki.content %>
</div>

<%= erb :'wiki/contribution_list', locals: {contributions: @wiki.contributions} %>

<div class="wiki_buttons">
  <!-- add an edit button -->
  <form action="/wiki/<%= @wiki.id %>" method="POST">
    <input type="hidden" name="_method" value="DELETE">
    <input type="submit" value="Delete!">
  </form>
</div>